<% members.each do |member| %>
  <p><strong>Name:</strong> <%= member.name %>
  	<%= link_to "(view)", member %>
  </p>
  <p><strong>Age:</strong> <%= member.age %></p>
  <p><strong>Company:</strong> <%= member.company %></p>
  <p><strong>Money:</strong> <%= member.money %></p>
  <% if member.recruiter %>
    <p><strong>Recruiter:</strong> <%= member.recruiter.name %></p>
  <% end %>
  <hr>
<% end %>